{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
        "cargoes_categories",
        "shiftworks",
        "priority"
    ],
    "properties": {
        "cargoes_categories": {
            "$id": "#/properties/cargoes_categories",
            "type": "array",
            "title": "The Cargoes_categories Schema",
            "items": {
                "$id": "#/properties/cargoes_categories/items",
                "type": "object",
                "title": "The Items Schema",
                "required": [
                    "ID",
                    "segment",
                    "unit",
                    "typical_amount_range",
                    "assignation_preference"
                ],
                "properties": {
                    "ID": {
                        "$id": "#/properties/cargoes_categories/items/properties/ID",
                        "type": "string",
                        "title": "The Id Schema",
                        "default": "",
                        "examples": [
                            "CargoCat1"
                        ],
                        "pattern": "^(.*)$"
                    },
                    "segment": {
                        "$id": "#/properties/cargoes_categories/items/properties/segment",
                        "type": "string",
                        "title": "The Segment Schema",
                        "default": "",
                        "examples": [
                            "Segment1"
                        ],
                        "pattern": "^(.*)$"
                    },
                    "unit": {
                        "$id": "#/properties/cargoes_categories/items/properties/unit",
                        "type": "string",
                        "title": "The Unit Schema",
                        "default": "",
                        "examples": [
                            "Tons"
                        ],
                        "pattern": "^(.*)$"
                    },
                    "typical_amount_range": {
                        "$id": "#/properties/cargoes_categories/items/properties/typical_amount_range",
                        "type": "array",
                        "title": "The Typical_amount_range Schema",
                        "items": {
                            "$id": "#/properties/cargoes_categories/items/properties/typical_amount_range/items",
                            "type": "integer",
                            "title": "The Items Schema",
                            "default": 0,
                            "examples": [
                                500,
                                10000
                            ]
                        }
                    },
                    "assignation_preference": {
                        "$id": "#/properties/cargoes_categories/items/properties/assignation_preference",
                        "type": "array",
                        "title": "The Assignation_preference Schema",
                        "items": {
                            "$id": "#/properties/cargoes_categories/items/properties/assignation_preference/items",
                            "type": "object",
                            "title": "The Items Schema",
                            "required": [
                                "priority",
                                "direction",
                                "dock_ID",
                                "supply_chain_ID"
                            ],
                            "properties": {
                                "priority": {
                                    "$id": "#/properties/cargoes_categories/items/properties/assignation_preference/items/properties/priority",
                                    "type": "integer",
                                    "title": "The Priority Schema",
                                    "default": 0,
                                    "examples": [
                                        1
                                    ]
                                },
                                "direction": {
                                    "$id": "#/properties/cargoes_categories/items/properties/assignation_preference/items/properties/direction",
                                    "type": "string",
                                    "title": "The Direction Schema",
                                    "default": "",
                                    "examples": [
                                        "unloading"
                                    ],
                                    "pattern": "^(.*)$"
                                },
                                "dock_ID": {
                                    "$id": "#/properties/cargoes_categories/items/properties/assignation_preference/items/properties/dock_ID",
                                    "type": "string",
                                    "title": "The Dock_id Schema",
                                    "default": "",
                                    "examples": [
                                        "Dock_1"
                                    ],
                                    "pattern": "^(.*)$"
                                },
                                "supply_chain_ID": {
                                    "$id": "#/properties/cargoes_categories/items/properties/assignation_preference/items/properties/supply_chain_ID",
                                    "type": "string",
                                    "title": "The Supply_chain_id Schema",
                                    "default": "",
                                    "examples": [
                                        "SC1"
                                    ],
                                    "pattern": "^(.*)$"
                                }
                            }
                        }
                    }
                }
            }
        },
        "shiftworks": {
            "$id": "#/properties/shiftworks",
            "type": "array",
            "title": "The Shiftworks Schema",
            "items": {
                "$id": "#/properties/shiftworks/items",
                "type": "object",
                "title": "The Items Schema",
                "required": [],
                "properties": {
                    "ID": {
                        "$id": "#/properties/shiftworks/items/properties/ID",
                        "type": "string",
                        "title": "The Id Schema",
                        "default": "",
                        "examples": [
                            "typical"
                        ],
                        "pattern": "^(.*)$"
                    },
                    "value": {
                        "$id": "#/properties/shiftworks/items/properties/value",
                        "type": "array",
                        "title": "The Value Schema",
                        "items": {
                            "$id": "#/properties/shiftworks/items/properties/value/items",
                            "type": "object",
                            "title": "The Items Schema",
                            "required": [
                            ],
                            "properties": {
                                "Monday": {
                                    "$id": "#/properties/shiftworks/items/properties/value/items/properties/Monday",
                                    "type": "array",
                                    "title": "The Monday Schema",
                                    "items": {
                                        "$id": "#/properties/shiftworks/items/properties/value/items/properties/Monday/items",
                                        "type": "string",
                                        "title": "The Items Schema",
                                        "default": "",
                                        "examples": [
                                            "8>12",
                                            "14>17"
                                        ],
                                        "pattern": "^(.*)$"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "priority": {
            "$id": "#/properties/priority",
            "type": "array",
            "title": "The Priority Schema",
            "items": {
              "$id": "#/properties/priority/items",
              "type": "string",
              "title": "The Items Schema",
              "default": "",
              "examples": [
                  "I.CLINKER",
                  "HASARDOUS",
                  "E.UNKNOWN",
                  "FIFO"
              ]
          }
        },
        "assignment": {
            "$id": "#/properties/assignment",
            "type": "object",
            "title": "The Assignment Schema"
        },
        "validity": {
            "$id": "#/properties/validity",
            "type": "object",
            "title": "The Validity Schema"
        }
    }
}
